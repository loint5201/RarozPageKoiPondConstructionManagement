<!-- Start Service Area -->
<section class="htc__service__area bg__white ptb--100" style="padding-bottom: 0">
    <div class="container">
        <div class="row">
            <div class="service__section__wrap clearfix">
                @foreach (var sevice in Model.MaintenanceService)
                {
                    <!-- Start Single Service -->
                    @* col-lg-3 col-sm-4 col-xs-12 *@
                    <div class="col-sm-4" style="margin-bottom: 16px">
                        <div class="service foo">
                            <div class="service__thumb">
                                <a asp-page="/Service/Index" asp-route-id="@sevice.ServiceId">
                                    <img src="~/uploads/@sevice.ServiceImage" alt="@sevice.ServiceName" width="300px" height="220px">
                                </a>
                                <div class="service__hover">
                                    <div class="service__action">
                                        <a asp-page="/Service/Index" asp-route-id="@sevice.ServiceId">Xem chi tiết</a>
                                    </div>
                                </div>
                            </div>
                            <div class="service__details">
                                <h2><a asp-page="/Service/Index" asp-route-id="@sevice.ServiceId">@sevice.ServiceName</a></h2>
                            </div>
                        </div>
                    </div>
                    <!-- End Single Service -->
                }
            </div>
        </div>
    </div>
</section>
<!-- End Service Area -->
